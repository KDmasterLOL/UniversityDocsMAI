doctype html
html(lang="en" data-theme="dark")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible" content="IE=edge")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        link(rel='stylesheet' href='../styles.css')
        script.
            MathJax = {
            loader: {load: ['[tex]/physics','[tex]/cancel']},
            tex: {
                packages: {'[+]': ['physics','cancel']},
                inlineMath: [['$', '$'], ['\\(', '\\)']],
                macros : {
                    //- dvp: ["{ \\dfrac{\\partial{#1}}{\\partial{#2}} }",2],
                    dbd: ["{ \\frac{\\Delta{#1}}{\\Delta{#2}}}",2],
                    const: ["{\\operatorname{const}}"],
                    oiint: ["{\\unicode{x222F}}"],
                    oiiint: ["{\\unicode{x2230}}"],
                    EMF: ["{\\mathscr{E}}"],
                    def: ["{\\operatorname{def}}"],
                    series: ['{\\sum_{n=#1}^{+\\infty}}', 1, '1'],
                    Nseries: ['{\\sum_{n=#1}^{N}}', 1, '1'],
                    def_by: ["{\\overset {\\operatorname{def}}} {#1}",1],
                    euint: ["{\\int #2 \\dd #1} = #3 + C", 3, 'x'],
                    bvec: ["{\\overrightarrow {#1}}", 1],
                    def: ["{\\text{≝}}"],
                    if: ["{\\text{Если }}"],

                }
            },
            };
        script(type="text/javascript" id="MathJax-script" async src="https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js")
        script(type="module" src="../script.js")
        title University document
        block scripts
    body 
        header 
            button(type="button")#switch-theme
                svg(id="svg-1" viewBox = "0 0 150 150" height=100)
                    circle(id="animated-circle" cx="75" cy="75" r="50")
            nav
                a(href="../engineering_graphics/engineering_graphics.html") Инженерная графика
                a(href="../math_analysis/math_analysis.html") Математический анализ
                a(href="../physics/physics.html") Физика 
                a(href="../linear_programing/linear_programing.html") Линейное программирование
                a(href="../info_university/info_university.html") Инфо Университет


